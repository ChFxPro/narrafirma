        },
        {
            "id": "elicitingQuestion_type",
            "valueType": "dictionary",
            "valueOptions": [
                "what happened",
                "directed question",
                "undirected questions",
                "point in time",
                "event",
                "extreme",
                "surprise",
                "people, places, things",
                "fictional scenario",
                "other"
            ],
            "displayType": "checkboxes",
            "displayName": "Type",

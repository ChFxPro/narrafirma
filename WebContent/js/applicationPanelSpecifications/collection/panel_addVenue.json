            "id": "venue_name",
            "dataType": "string",
            "required": true,
            "displayType": "text",
            "displayName": "Name",
            "displayPrompt": "Please give this venue plan a name."
        },
        {
            "id": "venue_primary_type",
            "dataType": "string",
            "dataOptions": [
                "individual interviews",
                "group interviews",
                "peer interviews",
                "group story sessions",
                "surveys",
                "journals",
                "narrative incident reports",
                "gleaned stories",
                "other"
